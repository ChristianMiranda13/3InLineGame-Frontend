{
  "name": "3inline-react-app",
  "version": "1.0.0",
  "private": true,
  "description": "game of 3 in line",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev-server-help": "webpack-dev-server --help",
    "start": "webpack-dev-server --config=webpack.config.dev.js --debug --devtool cheap-module-source-map --history-api-fallback --output-pathinfo --hot --inline --progress --colors --port 3004 --open --host 0.0.0.0",
    "build": "webpack -p --progress --config=webpack.config.dev.js --mode development --env dev",
    "lint": "tsc --noEmit && tslint -c tslint.json -p tsconfig.json '**/*.ts{,x}' -e '**/node_modules/**'",
    "deleteJsFiles": "cd src/ && find . -name '*.js' -type f -delete && find . -name '*.js.map' -type f -delete",
    "refreshModules": "rm -rf node_modules && rm -rf ~/.rncache && rm -rf yarn.lock && yarn cache clean && yarn install"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
